{
  "name": "mcp-clickup",
  "version": "1.0.1",
  "description": "A Model Context Protocol server for the ClickUp API.",
  "main": "index.ts",
  "type": "module",
  "bin": {
    "mcp-clickup": "dist/index.js"
  },
  "files": [
    "dist"
